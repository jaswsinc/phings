<?xml version="1.0" encoding="UTF-8" ?>

<project>

  <!-- ============================================  -->
  <!-- Pattern set matching `export-ignore` directives in our `.gitattributes` file. -->
  <!-- ============================================  -->

  <patternSet id="_git_export_ignore_pattern_set">

    <!-- ——————————————————————————————————————————— -->
    <!-- This follows `.gitignore` almost exactly. -->
    <!-- ——————————————————————————————————————————— -->

    <!-- Local -->

    <exclude pattern=".~**" />
    <exclude pattern="**/.~**" />

    <!-- Logs -->

    <exclude pattern="*.log" />
    <exclude pattern="**/*.log" />

    <!-- Backups -->

    <exclude pattern="*~" />
    <exclude pattern="**/*~" />

    <exclude pattern="*.bak" />
    <exclude pattern="**/*.bak" />

    <!-- Node -->

    <exclude pattern="node_modules/**" />
    <exclude pattern="**/node_modules/**" />

    <!-- SASS -->

    <exclude pattern=".sass-cache/**" />
    <exclude pattern="**/.sass-cache/**" />

    <!-- IntelliJ -->

    <exclude pattern=".idea/**" />
    <exclude pattern="**/.idea/**" />

    <!-- Sublime -->

    <exclude pattern=".sublime-workspace" />
    <exclude pattern="**/.sublime-workspace" />

    <exclude pattern=".sublime-project" />
    <exclude pattern="**/.sublime-project" />

    <!-- CTags -->

    <exclude pattern=".tags" />
    <exclude pattern="**/.tags" />

    <exclude pattern=".ctags" />
    <exclude pattern="**/.ctags" />

    <!-- VCS -->

    <exclude pattern=".git/**" />
    <exclude pattern="**/.git/**" />

    <exclude pattern=".svn/**" />
    <exclude pattern="**/.svn/**" />

    <exclude pattern="_svn/**" />
    <exclude pattern="**/_svn/**" />

    <exclude pattern=".CVS/**" />
    <exclude pattern="**/.CVS/**" />

    <exclude pattern=".cvsignore" />
    <exclude pattern="**/.cvsignore" />

    <exclude pattern=".bzr/**" />
    <exclude pattern="**/.bzr/**" />

    <exclude pattern=".bzrignore" />
    <exclude pattern="**/.bzrignore" />

    <exclude pattern=".hg/**" />
    <exclude pattern="**/.hg/**" />

    <exclude pattern=".hgignore" />
    <exclude pattern="**/.hgignore" />

    <exclude pattern="SCCS/**" />
    <exclude pattern="**/SCCS/**" />

    <exclude pattern="RCS/**" />
    <exclude pattern="**/RCS/**" />

    <!-- PC Files -->

    <exclude pattern=".$RECYCLE.BIN/**" />
    <exclude pattern="**/.$RECYCLE.BIN/**" />

    <exclude pattern="Desktop.ini" />
    <exclude pattern="**/Desktop.ini" />

    <exclude pattern="Thumbs.db" />
    <exclude pattern="**/Thumbs.db" />

    <exclude pattern="ehthumbs.db" />
    <exclude pattern="**/ehthumbs.db" />

    <!-- Mac Files -->

    <exclude pattern=".AppleDB" />
    <exclude pattern="**/.AppleDB" />

    <exclude pattern=".AppleDouble" />
    <exclude pattern="**/.AppleDouble" />

    <exclude pattern=".AppleDesktop" />
    <exclude pattern="**/.AppleDesktop" />

    <exclude pattern=".com.apple.timemachine.donotpresent" />
    <exclude pattern="**/.com.apple.timemachine.donotpresent" />

    <exclude pattern="Network Trash Folder" />
    <exclude pattern="**/Network Trash Folder" />

    <exclude pattern="Temporary Items" />
    <exclude pattern="**/Temporary Items" />

    <exclude pattern=".LSOverride" />
    <exclude pattern="**/.LSOverride" />

    <exclude pattern=".Spotlight-V100" />
    <exclude pattern="**/.Spotlight-V100" />

    <exclude pattern=".VolumeIcon.icns" />
    <exclude pattern="**/.VolumeIcon.icns" />

    <exclude pattern=".TemporaryItems" />
    <exclude pattern="**/.TemporaryItems" />

    <exclude pattern=".fseventsd" />
    <exclude pattern="**/.fseventsd" />

    <exclude pattern=".DS_Store" />
    <exclude pattern="**/.DS_Store" />

    <exclude pattern=".Trashes" />
    <exclude pattern="**/.Trashes" />

    <exclude pattern=".apdisk" />
    <exclude pattern="**/.apdisk" />

    <exclude pattern="Icon\r" />
    <exclude pattern="**/Icon\r" />

    <exclude pattern="._**" />
    <exclude pattern="**/._**" />

    <!-- AWS -->

    <exclude pattern=".elasticbeanstalk/**" />
    <exclude pattern="**/.elasticbeanstalk/**" />

    <!-- Vagrant -->

    <exclude pattern=".vagrant/**" />
    <exclude pattern="**/.vagrant/**" />

    <!-- ——————————————————————————————————————————— -->
    <!-- These are in addition to what we have in `.gitignore`. -->
    <!-- ——————————————————————————————————————————— -->

    <!-- Assets (in WS projects) -->

    <exclude pattern="assets/**" />
    <exclude pattern="src/vendor/websharks/*/assets/**" />

    <!-- Demo Directories -->

    <exclude pattern="demo/**" />
    <exclude pattern="demos/**" />
    <exclude pattern="src/vendor/*/*/demo/**" />
    <exclude pattern="src/vendor/*/*/demos/**" />

    <!-- Doc Directories -->

    <exclude pattern="doc/**" />
    <exclude pattern="docs/**" />
    <exclude pattern="src/vendor/*/*/doc/**" />
    <exclude pattern="src/vendor/*/*/docs/**" />

    <!-- Test Directories -->

    <exclude pattern="test/**" />
    <exclude pattern="tests/**" />
    <exclude pattern="src/vendor/*/*/test/**" />
    <exclude pattern="src/vendor/*/*/tests/**" />

    <!-- TO-DO Lists -->

    <exclude pattern="TODO.md" />
    <exclude pattern="**/TODO.md" />

    <exclude pattern="TODO.mdown" />
    <exclude pattern="**/TODO.mdown" />

    <exclude pattern="TODO.markdown" />
    <exclude pattern="**/TODO.markdown" />

    <exclude pattern="TODO.txt" />
    <exclude pattern="**/TODO.txt" />

    <!-- README Files -->

    <exclude pattern="README.md" />
    <exclude pattern="**/README.md" />

    <exclude pattern="README.mdown﻿⁠⁠⁠⁠" />
    <exclude pattern="**/README.mdown﻿⁠⁠⁠⁠" />

    <exclude pattern="README.markdown﻿⁠⁠⁠⁠" />
    <exclude pattern="**/README.markdown﻿⁠⁠⁠⁠" />

    <exclude pattern="README.txt﻿⁠⁠⁠⁠" />
    <exclude pattern="**/README.txt﻿⁠⁠⁠⁠" />

    <!-- CHANGELOG Files -->

    <exclude pattern="CHANGELOG.md" />
    <exclude pattern="**/CHANGELOG.md" />

    <exclude pattern="CHANGELOG.mdown﻿⁠⁠⁠⁠" />
    <exclude pattern="**/CHANGELOG.mdown﻿⁠⁠⁠⁠" />

    <exclude pattern="CHANGELOG.markdown﻿⁠⁠⁠⁠" />
    <exclude pattern="**/CHANGELOG.markdown﻿⁠⁠⁠⁠" />

    <exclude pattern="CHANGELOG.txt﻿⁠⁠⁠⁠" />
    <exclude pattern="**/CHANGELOG.txt﻿⁠⁠⁠⁠" />

    <!-- Git Dotfiles -->

    <exclude pattern=".gitignore" />
    <exclude pattern="src/vendor/*/*/.gitignore" />

    <exclude pattern=".gitattributes" />
    <exclude pattern="src/vendor/*/*/.gitattributes" />

    <exclude pattern=".gitmodules" />
    <exclude pattern="src/vendor/*/*/.gitmodules" />

    <exclude pattern=".gitchange" />
    <exclude pattern="src/vendor/websharks/*/.gitchange" />

    <!-- GitHub Dir/Files -->

    <exclude pattern=".github/**" />
    <exclude pattern="src/vendor/*/*/.github/**" />

    <exclude pattern="CONTRIBUTING.md" />
    <exclude pattern="src/vendor/*/*/CONTRIBUTING.md" />

    <exclude pattern="ISSUE_TEMPLATE.md" />
    <exclude pattern="src/vendor/*/*/ISSUE_TEMPLATE.md" />

    <exclude pattern="PULL_REQUEST_TEMPLATE.md" />
    <exclude pattern="src/vendor/*/*/PULL_REQUEST_TEMPLATE.md" />

    <!-- GitHub + Jekyll Dirs/Files -->
    <!-- Note: `docs/` and `assets/` are also used by Jekyll. -->
    <!-- They aren't listed here because they are listed elsewhere. -->

    <exclude pattern="_config.yml" />
    <exclude pattern="src/vendor/*/*/_config.yml" />

    <exclude pattern="_layouts/**" />
    <exclude pattern="src/vendor/*/*/_layouts/**" />

    <exclude pattern="_includes/**" />
    <exclude pattern="src/vendor/*/*/_includes/**" />

    <exclude pattern="_sass/**" />
    <exclude pattern="src/vendor/*/*/_sass/**" />

    <!-- Makefiles -->

    <exclude pattern="Makefile" />
    <exclude pattern="src/vendor/*/*/Makefile" />

    <!-- Gulp Files -->

    <exclude pattern="gulpfile.js" />
    <exclude pattern="src/vendor/*/*/gulpfile.js" />

    <!-- Grunt Files -->

    <exclude pattern="Gruntfile" />
    <exclude pattern="src/vendor/*/*/Gruntfile" />

    <!-- Phings Directory -->

    <exclude pattern="phings/**" />
    <exclude pattern="src/vendor/*/*/phing/**" />
    <exclude pattern="src/vendor/*/*/phings/**" />

    <!-- Phing Build Files -->

    <exclude pattern="build.xml" />
    <exclude pattern="src/vendor/*/*/build.xml" />

    <exclude pattern="build-*.xml" />
    <exclude pattern="src/vendor/*/*/build-*.xml" />

    <exclude pattern=".build.props" />
    <exclude pattern="src/vendor/*/*/.build.props" />

    <!-- Phing Dot Build Files -->

    <exclude pattern=".build.php" />
    <exclude pattern="src/vendor/websharks/*/.build.php" />

    <exclude pattern=".build.*.php" />
    <exclude pattern="src/vendor/websharks/*/.build.*.php" />

    <exclude pattern="src/.build.php" />
    <exclude pattern="src/vendor/websharks/*/src/.build.php" />

    <exclude pattern="src/.build.*.php" />
    <exclude pattern="src/vendor/websharks/*/src/.build.*.php" />

    <exclude pattern="src/client-s/**/.build.php" />
    <exclude pattern="src/vendor/websharks/*/src/client-s/**/.build.php" />

    <exclude pattern="src/client-s/**/.build.*.php" />
    <exclude pattern="src/vendor/websharks/*/src/client-s/**/.build.*.php" />

    <exclude pattern="src/includes/**/.build.php" />
    <exclude pattern="src/vendor/websharks/*/src/includes/**/.build.php" />

    <exclude pattern="src/includes/**/.build.*.php" />
    <exclude pattern="src/vendor/websharks/*/src/includes/**/.build.*.php" />

    <!-- IDE Helpers -->

    <exclude pattern="_ide.php" />
    <exclude pattern="**/_ide.php" />

    <!-- Composer Files -->

    <exclude pattern="composer.json" />
    <exclude pattern="src/vendor/*/*/composer.json" />

    <exclude pattern="composer.lock" />
    <exclude pattern="src/vendor/*/*/composer.lock" />

    <!-- PHPUnit -->

    <exclude pattern="phpunit.xml" />
    <exclude pattern="src/vendor/*/*/phpunit.xml" />

    <exclude pattern="phpunit.xml.dist" />
    <exclude pattern="src/vendor/*/*/phpunit.xml.dist" />

    <!-- SASS-Related -->

    <exclude pattern="**.scss" />
    <exclude pattern="**/**.scss" />

    <exclude pattern="**.css.map" />
    <exclude pattern="**/**.css.map" />

    <!-- Bourbon (in WS projects) -->

    <exclude pattern="src/client-s/**/bourbon/**" />
    <exclude pattern="src/vendor/websharks/*/src/client-s/**/bourbon/**" />

    <!-- Box / Phar -->

    <exclude pattern="box.json" />
    <exclude pattern="src/vendor/*/*/box.json" />

    <exclude pattern=".box.json" />
    <exclude pattern="src/vendor/*/*/.box.json" />

    <exclude pattern="box.json.dist" />
    <exclude pattern="src/vendor/*/*/box.json.dist" />

    <exclude pattern=".box.json.dist" />
    <exclude pattern="src/vendor/*/*/.box.json.dist" />

    <!-- Drone -->

    <exclude pattern=".drone.*" />
    <exclude pattern="src/vendor/*/*/.drone.*" />

    <!-- Circle CI -->

    <exclude pattern="circle.yml" />
    <exclude pattern="src/vendor/*/*/circle.yml" />

    <exclude pattern=".circle.yml" />
    <exclude pattern="src/vendor/*/*/.circle.yml" />

    <!-- Travis -->

    <exclude pattern=".travis.*" />
    <exclude pattern="src/vendor/*/*/.travis.*" />

    <exclude pattern=".travis-*" />
    <exclude pattern="src/vendor/*/*/.travis-*" />

    <!-- Coverage -->

    <exclude pattern=".coveralls.*" />
    <exclude pattern="src/vendor/*/*/.coveralls.*" />

    <exclude pattern=".scrutinizer.*" />
    <exclude pattern="src/vendor/*/*/.scrutinizer.*" />

    <!-- PHP Code Sniffer -->

    <exclude pattern=".php_cs" />
    <exclude pattern="src/vendor/*/*/.php_cs" />

    <!-- Post-Receive -->

    <exclude pattern="post-receive" />
    <exclude pattern="src/vendor/websharks/*/post-receive" />

    <!-- PHAR Stub -->

    <exclude pattern="src/includes/phar-stub.php" />
    <exclude pattern="src/vendor/websharks/*/src/includes/phar-stub.php" />

    <!-- PsySH Stub -->

    <exclude pattern="src/includes/psysh-stub.php" />
    <exclude pattern="src/vendor/websharks/*/src/includes/psysh-stub.php" />

  </patternSet>

</project>
